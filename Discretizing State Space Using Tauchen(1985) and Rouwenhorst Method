    "# 2. Value Function Iteration with Aggregate Uncertainty"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8692b802",
   "metadata": {},
   "source": [
    "## Model: Standard RBC with Divisible Labor"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 188,
   "id": "5adaca0a",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sympy.functions.elementary.exponential import exp\n",
    "from sympy.functions.elementary.exponential import log\n",
    "from sympy import Symbol\n",
    "from sympy import solve\n",
    "from scipy.optimize import fminbound\n",
    "from scipy.optimize import brentq\n",
    "from scipy.interpolate import interp1d\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import statsmodels.api as sm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 189,
   "id": "36b7c2ef",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Assigning Parameteres\n",
    "β = 0.99; θ = 0.36; δ = 0.025; ρ=0.95; σ = 0.007\n",
    "\n",
    "# Defining the basic functions\n",
    "def u(c,l, μ):\n",
    "    return log(c) + μ * log(l)\n",
    "\n",
    "def f(k, h, θ = θ):\n",
    "    return (k**θ) * (h**(1-θ))\n",
    "\n",
    "def c(z, k, kp,h, θ = θ):\n",
    "    return exp(z)*f(k, h) + (1-δ)*k - kp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 190,
   "id": "fc8f27d5",
   "metadata": {},
   "outputs": [],
   "source": [
    "k = Symbol('k') ; z = Symbol('z'); h = Symbol('h'); kp = Symbol('kp')\n",
    "\n",
    "# Setting the Steady State Level\n",
    "\n",
    "z_bar = 0; h_bar = 1/3\n",
    "\n",
    "\n",
    "# Setting Euler Equations\n",
    "EE1 = β * (exp(z_bar) * f(k,h).diff(k).subs({h:h_bar}) + 1 - δ ) -1\n",
    "k_bar = float(solve(EE1, k)[0])\n",
    "\n",
    "μ = Symbol('μ')\n",
    "c_bar = c(z_bar, k_bar, h_bar, k_bar)\n",
    "\n",
    "EE2 = (1 / c_bar) * exp(z_bar) * f(k, h).diff(h).subs({k:k_bar, h:h_bar}) - μ/(1-h_bar)\n",
    "μ = float(solve(EE2, μ)[0])\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 191,
   "id": "f0019773",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining the basic functions in the Steady State\n",
    "def u(c,l, μ = μ):\n",
    "    return log(c) + μ * log(l)\n",
    "\n",
    "def f(k, h=h_bar, θ = θ):\n",
    "    return (k**θ) * (h**(1-θ))\n",
    "\n",
    "def c(z, k, kp, h=h_bar, θ=θ):\n",
    "    return exp(z) * f(k,h) + (1-δ)*k - kp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 192,
   "id": "4f9fc9c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining Maximization Funcion V on interval [a,b]\n",
    "def maximum(V,a,b):\n",
    "    return float(V(fminbound(lambda x: -V(x),a,b)))\n",
    "\n",
    "# Defining Minimization Funcion V on interval [a,b]\n",
    "def minimum(V,a,b):\n",
    "    return float(V(fminbound(lambda x: V(x),a,b)))\n",
    "\n",
    "# Defining the Maximizer of Function V on interval [a,b]\n",
    "def maximizer(V,a,b):\n",
    "    return float(fminbound(lambda x: -V(x),a,b))\n",
    "\n",
    "# Defining the Minimizer of function V on interval [a,b]\n",
    "def minimizer(V,a,b):\n",
    "    return float(fminbound(lambda x: V(x),a,b))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 216,
   "id": "cb83c226",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Setting the grid of state vairable over which the function will be approximated\n",
    "grid_min = k_bar * 0.9\n",
    "grid_max = f(k_bar) + (1-δ) * k_bar\n",
    "grid_size = 30\n",
    "grid = np.linspace(grid_min, grid_max, grid_size)"
   ]
