    "zs = z_proc \n",
    "Π = Π\n",
    "\n",
    "# Gusse the initial Value Function\n",
    "Vs = []\n",
    "for i in range(np.size(zs)):\n",
    "    Vs.append(interp1d(grid, np.log(grid)))"
   ]
